#include <stdio.h>
#include<string.h>

int main() 
{
    char x[100],s[100];
    int j,k,m;
    printf("Enter the word ");
    fgets(x,100,stdin);
    int i=(strlen(x)-1)/2;
    m=i;
    while(x[i+1]!='\0')
    {
        s[j]=x[i];
        i++;
        j++;
        printf("%s\n",s);

    }
    for(k=0;k<m;k++)
    {
        s[j]=x[k];
        printf("%s\n",s);
        j++;
    }
}
